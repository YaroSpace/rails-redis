<p>Game: <%= link_to @game.name, team_path(@game)  %> </p>
<br>
<% @game.channels.each do |channel|  %>
  <p>Channel: <%= link_to channel.name, channel_path(channel)  %> </p>
  <p>Username: <%= channel.user.name  %></p>
  <p>Game: <%= channel.game.name  %></p>
  <p><b>Watchers: </b><%= @channel_watchers[channel.id]  %></p/>
<% end  %>
<hr>
